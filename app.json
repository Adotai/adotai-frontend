{
  "expo": {
    "name": "adotai-frontend",
    "slug": "adotai-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adotai-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/adotai-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#AD334A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adotai.mobile"   
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adotai-logo.png",
        "backgroundColor": "#AD334A"
      },
      "package": "com.adotai.mobile"           
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
    }
  }
}
